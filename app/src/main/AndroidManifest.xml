<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eshopping">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fashion_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fashion_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.EShopping">
        <activity
            android:name=".itemselected"
            android:exported="false" />
        <activity
            android:name=".cart"
            android:exported="false" />
        <activity
            android:name=".ActivityStart"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">

        android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <provider android:name="ShopCart" android:authorities="com.example.MyApplication.ShoppingCart"/>

    </application>

</manifest>